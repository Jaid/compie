{{#each imports}}
import {{import}} from "{{from}}"
{{/each}}

/**
  * @typedef {Object} Props
      {{#each props}}
      {{#if jsdocType}}
  * @prop { {{~jsdocType~}} } {{propName}}
      {{/if}}
      {{/each}}
  */

/**
  * @class
  * @extends {React.Component<Props>}
  */
export default class {{className}} extends React.Component {

  static propTypes = {
    {{#each props}}
    {{#if propType}}
    {{propName}}: {{propType}},
    {{/if}}
    {{/each}}
  }

  render() {
    const content = <span>{{content}}</span>
    return <{{wrapperTag}} className={
    {{~#if isPage~}}
      css.container
    {{~else~}}
      classnames(css.container, this.props.className)
    {{~/if~}}
      }>
      {content}
    </{{wrapperTag}}>
  }

}