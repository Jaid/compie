{{#each imports}}
import {{import}} from "{{from}}"
{{/each}}

/**
  * @typedef \{{
     {{#each props}}
  *  {{propName}}: {{jsdocType}},
     {{/each}}
  * }} Props
  */

/**
  * @class
  * @extends {React.Component<Props>}
  */
export default class {{className}} extends React.Component {

  static propTypes = {
    {{#each props}}
    {{propName}}: {{propType}},
    {{/each}}
  }

  render() {
    return <{{wrapperTag}} className={classnames(css.container, this.props.className)}>
      {{content}}
    </{{wrapperTag}}>
  }

}